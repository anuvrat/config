# ~/.config/git/config.local
# Machine-specific identity and signing config (not tracked in git)
# Copy this file to ~/.config/git/config.local and fill in your values.

[user]
	name = Your Name
	email = your@email.com
	# signingkey = ssh-ed25519 AAAA...

